<div class="row">
    <div class="col-md-12">

        <h3>Current User Claims</h3>

        @User.Identity.Name<br/>

        <table class="table">
            <thead>
                <tr>
                    <th>Claim></th><th>Value</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var claim in User.Claims)
                {
                    <tr>
                        <td>@claim.Type</td>
                        <td>@claim.Value</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>